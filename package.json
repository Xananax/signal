{
  "name": "gameover",
  "version": "0.0.1",
  "description": "Gamedev/UIdev framework",
  "main": "./main.js",
  "scripts": {
    "test": "karma start",
    "test-integration":"karma start --single-run",
    "compile": "NODE_ENV=production webpack",
    "build":"npm run todo && npm run test-integration && npm run compile",
    "start": "webpack-dev-server --hot --progress --colors --content-base public/",
    "todo": "./generate-todo.sh",
    "typings":"rm -f index.d.ts && tsc --module amd --declaration --outFile _module.js ./src/utils.ts && npm run typings-clean && mv _module.d.ts index.d.ts",
    "typings-clean":"rm -rf _module.js _module.js.map"
  },
  "typings":"./index.d.ts",
  "author": "xananax",
  "license": "MIT",
  "devDependencies": {
    "autoprefixer": "^6.3.6",
    "babel-core": "^6.7.7",
    "babel-loader": "^6.2.4",
    "babel-plugin-react-transform": "^2.0.2",
    "babel-preset-es2015": "^6.6.0",
    "babel-register": "^6.7.2",
    "chai": "^3.5.0",
    "css-loader": "^0.23.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "karma": "^0.13.22",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^0.2.3",
    "karma-mocha": "^0.2.2",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-spec-reporter": "0.0.26",
    "karma-typescript-preprocessor2": "^1.1.1",
    "karma-webpack": "^1.7.0",
    "less": "^2.6.1",
    "less-loader": "^2.2.3",
    "mocha": "^2.4.5",
    "postcss-loader": "^0.8.2",
    "precss": "^1.4.0",
    "source-map-loader": "^0.1.5",
    "style-loader": "^0.13.1",
    "stylus": "^0.54.4",
    "ts-loader": "^0.8.2",
    "typescript": "^1.8.10",
    "typings": "^0.8.1",
    "webpack": "^1.13.0",
    "webpack-dev-server": "^1.14.1"
  },
  "dependencies": {}
}
